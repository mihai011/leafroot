        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "env": { "ENV_FILE": ".env_user"},
            "name": "Python: tests",
            "type": "python",
            "request": "launch",
            "module": "pytest",
            "args": [
                "-s",
                "tests/test_user_controllers.py"
            ],
        },
        {
            "env": { "ENV_FILE": ".env_user"},
            "name": "Python: FastAPI",
            "type": "python",
            "request": "launch",
            "module": "uvicorn",
            "cwd": "${workspaceFolder}",
            "args": [
                "app.app:app",
                "--host",
                "0.0.0.0",
                "--port",
                "80",
                "--reload",
            ],
            "jinja": true
        }
    ]
}
